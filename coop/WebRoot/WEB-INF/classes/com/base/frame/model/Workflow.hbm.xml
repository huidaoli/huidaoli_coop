<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_Workflow" name="com.base.frame.model.Workflow">
     <id name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="realName"/>
    <property name="processDefPath"/>
    <property name="processImagePath"/>
    <property name="processDefCord"/>
  </class>
</hibernate-mapping>
