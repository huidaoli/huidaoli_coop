<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class table="T_SysConfig" name="com.base.frame.model.SysConfig">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="key" column="keycol"/>
		<property name="value" column="valuecol"/>
		<property name="keyword" column="keywordcol"/>
		<property name="group" column="groupcol"/>
		<property name="editor" />
		

	</class>
</hibernate-mapping>
